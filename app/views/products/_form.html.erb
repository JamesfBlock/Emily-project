<%= simple_form_for(@product) do |f| %>
  <%= f.input :room_id, collection: Room.all %>
  <%= f.input :name, label: "Product Name" %>
  <%= f.input :company_id, collection: Company.all %>
  <%= f.input :description %>
  <%= f.input :URL %>
  <%= f.input :image_url %>
  <%= f.input :price %>
  <%= f.input :featured %>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
