<%= simple_form_for(@product) do |f| %>
  <%= f.input :name %>
  <%= f.input :company %>
  <%= f.input :description %>
  <%= f.input :emzy_comment %>
  <%= f.input :url %>
  <%= f.input :image_url %>
  <%= f.input :price %>
  <%= f.input :featured %>
  <%= f.input :category_id, collection: @category %>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
